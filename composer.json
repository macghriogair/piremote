{
  "name": "macghriogair/piremote",
  "description": "Lumen based web interface for RasPi's power plug daemon",
  "keywords": [
    "raspberrypi",
    "plug",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Patrick Mac Gregor",
      "email": "macghriogair@gmx.net"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.5.9",
    "laravel/lumen-framework": "5.2.*",
    "vlucas/phpdotenv": "~2.2"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  }
}
